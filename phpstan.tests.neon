includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon
parameters:
    ignoreErrors:
        - '#Parameter \#1 \$params of static method League\\Uri\\Component\\Query::createFromParams\(\) expects array\|Traversable, string given\.#'
        - '#Parameter \#1 \$offsets of method League\\Uri\\Component\\Host::withoutLabels\(\) expects array\<int\>, array\<int, string\> given\.#'
        - '#Parameter \#1 \$offsets of method League\\Uri\\Component\\HierarchicalPath::withoutSegments\(\) expects array\<int\>, array\<int, string\> given\.#'
        - '#Parameter \#1 \$content of method League\\Uri\\Component\\UserInfo::withContent\(\) expects string\|null, DateTime\|false given\.#'
        - '#Parameter \#2 \$value of method League\\Uri\\Contract\\QueryInterface::appendTo\(\) expects string\|null, int given\.#'
        - '#Parameter \#2 \$value of method League\\Uri\\Component\\Query::appendTo\(\) expects string\|null, int given\.#'
        - '#Parameter \#2 \$value of method League\\Uri\\Component\\Query::appendTo\(\) expects string\|null, array<int, string> given\.#'
        - '#Parameter \#2 \$value of method League\\Uri\\Contract\\QueryInterface::withPair\(\) expects string, int given\.#'
    reportUnmatchedIgnoredErrors: false